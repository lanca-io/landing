.info-card {
	display: flex;
	flex-direction: column;
	border-radius: var(--space-5);
	padding: var(--space-5);
	gap: var(--space-5);
	background-color: var(--color-grey-25);
	width: 207px;
	height: 132px;
}

.info-card__heading {
	display: flex;
	flex-direction: row;
	align-items: center;
	gap: var(--space-3);
}
