.separator-wrap {
	display: flex;
	position: relative;
	flex-direction: row;
	align-items: center;
	justify-content: center;
	width: 100%;
	height: 32px;
}

.separator {
	width: 100%;
	height: 1px;
	background-color: var(--color-grey-100);
}

.direction-icon {
	width: 32px;
	height: 32px;
	border-radius: var(--space-3);
	margin: 0 var(--space-4);
	padding: var(--space-2);
	background-color: var(--color-grey-100);
}
