{
  "name": "landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "npx vite",
    "dev": "npx vite",
    "build": "npx vite build",
    "format": "npx prettier --write \"src/**/*.{ts,tsx,md,css,pcss}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" && stylelint \"src/**/*.{css,pcss}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix && stylelint \"src/**/*.{css,pcss}\" --fix",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "husky": {
		"hooks": {
			"pre-commit": "npm run format && npm run lint:fix && git add -A .",
			"pre-push": "npm run test"
		}
	},
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^9.17.0",
    "eslint-config-love": "^114.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^8.0.3",
    "precss": "^4.0.0",
    "prettier": "^3.2.5",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-standard": "^20.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^5.1.6",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-stylelint": "^5.3.1"
  }
}
